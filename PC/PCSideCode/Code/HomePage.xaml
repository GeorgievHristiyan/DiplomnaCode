<Page x:Class="Code.HomePage"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:Code"
      mc:Ignorable="d" 
      d:DesignHeight="300" d:DesignWidth="300"
      Title="HomePage">

    <Grid Name="HomeGrid" VerticalAlignment="Center" HorizontalAlignment="Center">
        <Button x:Name="SettingsButton" Content="Settings" HorizontalAlignment="Left" Margin="37,134,0,0" VerticalAlignment="Top" Width="97" Height="31" BorderThickness="1,1,2,1" Click="SettingsButton_Click"/>
        <Button x:Name="StartPrintingButton" Content="Start printing" HorizontalAlignment="Left" Margin="180,134,0,0" VerticalAlignment="Top" Width="97" Height="31" BorderThickness="1,1,2,1" Click="StartPrintingButton_Click"/>
        <Label x:Name="label" Content="Welcome to Zeta - 3D automation" HorizontalAlignment="Left" Margin="26,59,0,0" VerticalAlignment="Top" FontSize="16" FontWeight="Bold"/>

    </Grid>
</Page>
